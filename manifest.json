{
  "manifest_version" : 2, 
  "name" : "Movie Fox", 
  "description" : "The most popular movie and TV shows in your browser's default tab", 
  "version" : "1.0", 
  "author" : "Ravi Kumar Singh", 
  "browser_action" : {
    "default_icon" : "tab-icon.png", 
    "default_title" : "Have a good day", 
    "default_popup" : "popup.html"
  },
  "chrome_url_overrides" : {
    "newtab" : "newtab.html"
  }, 
  "permissions" : ["activeTab"]
}